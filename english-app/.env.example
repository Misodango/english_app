# Vue CLI uses VUE_APP_* variables at build time.
VUE_APP_GITHUB_OWNER=your-github-owner
VUE_APP_GITHUB_REPO=your-github-repo
VUE_APP_GITHUB_TOKEN=replace-with-github-token

VUE_APP_FIREBASE_API_KEY=replace-with-firebase-api-key
VUE_APP_FIREBASE_AUTH_DOMAIN=replace-with-firebase-auth-domain
VUE_APP_FIREBASE_PROJECT_ID=replace-with-firebase-project-id
VUE_APP_FIREBASE_STORAGE_BUCKET=replace-with-firebase-storage-bucket
VUE_APP_FIREBASE_MESSAGING_SENDER_ID=replace-with-firebase-messaging-sender-id
VUE_APP_FIREBASE_APP_ID=replace-with-firebase-app-id
VUE_APP_MAIL_ENDPOINT=replace-with-mail-endpoint

# Feature flags
VUE_APP_ENABLE_SETTINGS=true
